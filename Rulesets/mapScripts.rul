        - [6, 0, 1, 1]
        - [6, 1, 1, 1]
        - [0, 0, 1, 2]
        - [6, 0, 1, 2]
    - type: addBlock
      terrain: IDT_PLANE_HIJACKED
      size: [3, 2]
      executions: 1
      executionChances: 100
      rects:
        - [2, 0, 3, 2]
    - type: addBlock #North runway
      executions: 1
      executionChances: 100
      groups: 3
      rects:
        - [0, 0, 1, 1]
    - type: addBlock #North South
      executions: 1
      executionChances: 100
      groups: 4
      rects:
        - [0, 1, 1, 1]
    - type: addBlock #North runway
      executions: 1
      executionChances: 100
      groups: 3
